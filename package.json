{
  "name": "@storyhunt/wrapper-sdk",
  "version": "1.0.6",
  "author": "StoryHunt Org",
  "description": "StoryHunt SDK for interacting with storyhunt contracts",
  "license": "MIT",
  "repository": "https://github.com/0xstoryhunt/storyhunt-sdk.git",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "type": "module",
  "keywords": [
    "storyhunt",
    "dex",
    "sdk"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run ts-check && npm run build-lib",
    "build-lib": "tsup --config ./tsup.config.lib.ts",
    "test": "node --experimental-vm-modules ./node_modules/.bin/jest --verbose",
    "ts-check": "tsc --noEmit",
    "size": "size-limit",
    "prepare": "npm run build-lib"
  },
  "dependencies": {
    "@ethersproject/address": "^5.7.0",
    "@ethersproject/solidity": "^5.7.0",
    "@storyhunt/sdk-core": "^1.0.1",
    "@storyhunt/v3-sdk": "^1.0.3",
    "@wagmi/core": "^2.6.5",
    "ethers": "^6.13.4",
    "jsbi": "^3.2.5",
    "urql": "^4.2.1",
    "viem": "^2.7.15",
    "wagmi": "^2.12.25"
  },
  "devDependencies": {
    "@tanstack/query-core": "^5.62.7",
    "@tanstack/react-query": "^5.62.7",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "^7.12.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "prettier": "^3.3.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.3",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5"
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": true
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "provenance": false
  }
}
